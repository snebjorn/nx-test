{
  "version": 1,
  "projects": {
    "ng-lib": {
      "projectType": "library",
      "root": "libs/ng-lib",
      "sourceRoot": "libs/ng-lib/src",
      "prefix": "nx-test",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/ng-lib/tsconfig.lib.json",
            "project": "libs/ng-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/ng-lib/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/ng-lib/tsconfig.lib.json",
              "libs/ng-lib/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/ng-lib/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/ng-lib/jest.config.js",
            "tsConfig": "libs/ng-lib/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/ng-lib/src/test-setup.ts"
          }
        }
      },
      "schematics": {}
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "ng-lib"
}
